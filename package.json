{
  "name": "device-kit-ui",
  "version": "0.1.8",
  "private": true,
  "scripts": {
    "build-only": "vite build",
    "build": "run-p type-check \"build-only {@}\" --",
    "dev": "vite",
    "format": "prettier --write src/",
    "generate": "typia generate --input src/typia_src --output src/typia --project tsconfig.app.json",
    "lint": "eslint .",
    "prepare": "ts-patch install",
    "start": "node server.cjs",
    "test": "vitest",
    "ts-node": "TS_NODE_PROJECT=tsconfig.node.json ts-node",
    "type-check": "vue-tsc --build --force"
  },
  "type": "module",
  "dependencies": {
    "@sgpinkus/mdi-font": "^7.3.67",
    "@sgpinkus/my-vue-router": "^1.3.3",
    "axios": "^1.7.9",
    "d3": "^7.9.0",
    "dayjs": "^1.11.19",
    "express": "^4.22.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "pyodide": "^0.26.3",
    "uuid": "^13.0.0",
    "vue": "^3.5.27",
    "vuetify": "^3.11.6"
  },
  "devDependencies": {
    "@ryoppippi/unplugin-typia": "^2.6.5",
    "@tsconfig/node22": "^22.0.5",
    "@types/d3": "^7.4.3",
    "@types/express": "^5.0.6",
    "@types/lodash": "^4.17.23",
    "@types/md5": "^2.3.6",
    "@types/node": "^25.0.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.7.4",
    "sass": "^1.97.2",
    "terser": "^5.44.1",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.53.0",
    "typia": "^9.3.1",
    "vite": "^7.3.1",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^4.0.17",
    "vue-tsc": "^3.2.2"
  }
}

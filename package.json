{
  "name": "device-kit-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev-gh": "vite -c vite.config.gh-pages.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-gh": "run-p type-check \"build-only-gh {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only-gh": "vite build -c vite.config.gh-pages.ts",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "ts-node": "TS_NODE_PROJECT=tsconfig.node.json ts-node",
    "test": "vitest"
  },
  "dependencies": {
    "@sgpinkus/mdi-font": "^7.3.67",
    "@sgpinkus/my-vue-router": "^1.2.2",
    "axios": "^1.7.7",
    "d3": "^7.9.0",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "pyodide": "^0.26.3",
    "uuid": "^9.0.1",
    "vue": "^3.5.10",
    "vuetify": "^3.7.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.17.9",
    "@types/node": "^20.14.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "sass": "^1.79.4",
    "terser": "^5.36.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.0.21"
  }
}

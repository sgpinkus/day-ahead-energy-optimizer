{
  "name": "device-kit-ui",
  "version": "0.1.8",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev-gh": "vite -c vite.config.gh-pages.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-gh": "run-p type-check \"build-only-gh {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only-gh": "vite build -c vite.config.gh-pages.ts",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "ts-node": "TS_NODE_PROJECT=tsconfig.node.json ts-node",
    "test": "vitest",
    "prepare": "ts-patch install && typia patch",
    "start": "node server.cjs"
  },
  "type": "module",
  "dependencies": {
    "@sgpinkus/mdi-font": "^7.3.67",
    "@sgpinkus/my-vue-router": "^1.3.3",
    "axios": "^1.7.9",
    "d3": "^7.9.0",
    "dayjs": "^1.11.19",
    "express": "^5.2.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "pyodide": "^0.26.3",
    "uuid": "^13.0.0",
    "vue": "^3.5.26",
    "vuetify": "^3.11.6"
  },
  "devDependencies": {
    "@ryoppippi/unplugin-typia": "^2.6.5",
    "@tsconfig/node22": "^22.0.5",
    "@types/d3": "^7.4.3",
    "@types/express": "^5.0.6",
    "@types/lodash": "^4.17.23",
    "@types/md5": "^2.3.6",
    "@types/node": "^25.0.8",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.7.4",
    "sass": "^1.97.2",
    "terser": "^5.44.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.53.0",
    "typia": "^11.0.0",
    "vite": "^6.0.4",
    "vite-plugin-static-copy": "^2.2.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  }
}
